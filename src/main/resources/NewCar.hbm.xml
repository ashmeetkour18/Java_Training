<?xml version="1.0" encoding="UTF-8" ?>
<hibernate-mapping xmlns="http://www.hibernate.org/xsd/orm/hbm">

    <class name="Day8.NewCar" table="NewCar">
        <id name="id" type="java.lang.Integer">
            <column name="id"></column>
            <generator class="increment"/>
        </id>
        <property name="name" column="name" type="java.lang.String"/>
        <property name="speed" column="speed" type="java.lang.Double"/>
        <property name="horsePower" column="horsePower" type="java.lang.Float"/>
        <property name="maker" column="maker" type="java.lang.Character"/>
        <property name="isHatchBack" column="hatchBack" type="java.lang.Boolean"/>
        <property name="carNumber" column="carNumber" type="java.lang.Long"/>

    </class>
</hibernate-mapping>